{
    "defaultSeverity": "error",
    "extends":
    [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules":
    {
        "space-before-function-paren": true,
        "one-line": [false, "check-open-brace"],
        "no-console": false,
        "no-trailing-whitespace": false,
        "prefer-for-of": false,
        "ordered-imports": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
